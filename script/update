#!/bin/bash
WORKINGDIR=""
cd 
#cd $WORKINGDIR
sudo rm -r "superpowers"
sudo wget -q $1 -O "sup"
filename=$(unzip -qql "sup" | head -n1 | tr -s ' ' | cut -d' ' -f5-)
sudo unzip -q "sup"
sudo mv $filename "superpowers"
sudo rm -r "sup"
chmod -R 777 "superpowers"
cd "superpowers/app/server/"
forever stop start.js
forever start start.js
echo "Running !"
